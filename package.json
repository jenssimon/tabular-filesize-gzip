  },
  "devDependencies": {
    "@jenssimon/eslint-config-base": "5.4.1",
    "@jenssimon/eslint-config-typescript": "3.4.2",
    "@types/glob": "7.2.0",
    "@types/node": "17.0.23",
    "eslint": "8.15.0",
    "npm-run-all": "4.1.5",
    "typescript": "4.6.4"
  },
  "eslintConfig": {
    "extends": [
      "@jenssimon/base",
      "@jenssimon/typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
