{
  "name": "tabular-filesize-gzip",
  "version": "0.0.0-development",
  "description": "Show file sizes tabular including Gzip sizes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jenssimon/tabular-filesize-gzip"
  },
  "author": "Jens Simon <jens.simon80@googlemail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix ./",
    "build": "tsc",
    "prepare": "npm-run-all build",
    "ci-test": "run-s \"ci-test:*\"",
    "ci-test:lint": "eslint --ext .tsx,.ts,.jsx,.js ./",
    "ci-test:tsc": "tsc --noEmit",
    "commit": "cz",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "file",
    "size",
    "filesize",
    "gzip",
    "table"
  ],
  "dependencies": {
    "chalk": "^5.0.0",
    "filesize": "^8.0.7",
    "glob": "^8.0.0",
    "gzip-size": "^7.0.0",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "@jenssimon/eslint-config-base": "5.4.1",
    "@jenssimon/eslint-config-typescript": "3.4.2",
    "@types/glob": "7.2.0",
    "@types/node": "17.0.23",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.15.0",
    "husky": "8.0.1",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "semantic-release": "19.0.2",
    "typescript": "4.6.4"
  },
  "eslintConfig": {
    "extends": [
      "@jenssimon/base",
      "@jenssimon/typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "packageManager": "yarn@3.2.1",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
